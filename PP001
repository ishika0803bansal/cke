//pp001: Write a program to input a number and check it to be even or odd using if statement.
# include<stdio.h> 

int main(){ 

     int a;
     printf("enter a number:");
     scanf("%d" , &a);
     if (a%2==0) 
     {
         printf("even");
      }
    else
    { 
         printf("odd");
    }
  }
  
  
